{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Keyword Expander Agent Input",
  "type": "object",
  "properties": {
    "seed_keywords": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "A list of seed keywords to expand upon."
    },
    "num_variations": {
      "type": "integer",
      "description": "The number of keyword variations to generate.",
      "default": 10
    }
  },
  "required": ["seed_keywords"]
}